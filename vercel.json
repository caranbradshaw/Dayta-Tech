{
  "version": 2,
  "routes": [
    {
      "src": "/select-role",
      "dest": "/select-role"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "@next_public_app_url"
  },
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 60
    }
  },
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".next"
}
